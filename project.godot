config_version=5

[application]

config/name="Space N Stuff"
run/main_scene="res://Main/Main.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/stretch/scale=3.0

[editor_plugins]

enabled=PackedStringArray("res://addons/nklbdev.importality/plugin.cfg")

[importality]

temporary_files_directory_path="../../Temp"
aseprite_or_libre_sprite_command="../../Aseprite/Aseprite.exe"

